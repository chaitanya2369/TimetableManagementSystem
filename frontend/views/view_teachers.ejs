<!DOCTYPE html>
<html>
<head>
    <title>Teachers in <%= department %> Department</title>
    <link rel="stylesheet" type="text/css" href="/css/admin.css">
</head>
<body>
    <header>
        <div class="header">
            <div class="logo">
                <img src="/images/logo1.png" alt="Institution Logo">
            </div>
            <h1>Admin Dashboard</h1>
            <div class="login">
                <button><a href="/users/sign-out">Sign Out</a></button>
            </div>
        </div>
    </header>
    <main>
        <h1>Teachers in <%= department %> Department</h1>
        <ul>
            <% teachers.forEach(teacher => { %>
                <li><%= teacher.name %></li>
            <% }); %>
        </ul>
    </main>
    <footer>
        <div class="footer-content">
            <p>&copy; 2024 Indian Institute of Engineering Science and Technology, Shibpur. All rights reserved.</p>
        </div>
    </footer>
</body>
</html>
